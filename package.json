{
    "name": "vscode-slm",
    "displayName": "Slm",
    "description": "Sim language support for VS Code",
    "version": "1.0.0",
    "publisher": "mrmlnc",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "slm",
            "aliases": ["Slm"],
            "extensions": [".slm"],
            "configuration": "./slm.configuration.json"
        }],
        "grammars": [{
            "language": "slm",
            "scopeName": "text.slm",
            "path": "./syntaxes/slm.tmLanguage"
        }]
    }
}
